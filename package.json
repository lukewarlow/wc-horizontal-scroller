{
  "name": "wc-horizontal-scroller",
  "version": "0.7.1",
  "description": "Web component for a horizontal scroller",
  "keywords": [
    "web",
    "component",
    "front-end",
    "web-components",
    "web-component",
    "webcomponents",
    "webcomponent",
    "web components",
    "web component",
    "carousel",
    "scroller",
    "horizontal-scroller",
    "horizontal scroller",
    "TypeScript"
  ],
  "type": "module",
  "side-effects": false,
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.umd.cjs"
      }
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --declaration --emitDeclarationOnly --outDir dist"
  },
  "repository": "https://github.com/lukewarlow/wc-horizontal-scroller.git",
  "author": "Luke Warlow",
  "license": "MIT",
  "devDependencies": {
    "typescript": "5.7.3",
    "vite": "^6.1.0"
  },
  "dependencies": {
    "invokers-polyfill": "^0.4.6"
  }
}
